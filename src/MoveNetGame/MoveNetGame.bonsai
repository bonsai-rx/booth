              <Color>
                <Val0>255</Val0>
                <Val1>255</Val1>
                <Val2>255</Val2>
                <Val3>255</Val3>
              </Color>
              <LocationX>0.7037037</LocationX>
              <LocationY>0.7777778</LocationY>
